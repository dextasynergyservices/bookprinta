{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true
    },
    "transform": {
      "decoratorMetadata": true,
      "legacyDecorator": true
    },
    "target": "es2022",
    "keepClassNames": true,
    "baseUrl": ".",
    "paths": {}
  },
  "module": {
    "type": "es6",
    "strict": true,
    "noInterop": false,
    "resolveFully": true
  },
  "sourceMaps": true
}
