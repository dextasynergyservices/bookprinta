{
  "name": "@bookprinta/shared",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "bun": "./index.ts",
      "types": "./index.ts",
      "default": "./dist/index.js"
    },
    "./schemas/*.ts": {
      "bun": "./schemas/*.ts",
      "types": "./schemas/*.ts",
      "default": "./dist/schemas/*.js"
    },
    "./schemas/*": {
      "bun": "./schemas/*",
      "types": "./schemas/*",
      "default": "./dist/schemas/*.js"
    }
  },
  "scripts": {
    "build": "bun run build.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "zod": "^4.3.6"
  }
}
